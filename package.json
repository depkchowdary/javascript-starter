{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node sourceScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check lint:watch source:start test:watch",
    "source:start": "babel-node  sourceScripts/server.js",
    "security-check": "npm audit",
    "share": "npm-run-all --parallel start source:share",
    "source:share": "lt -8080",
    "lint": "esw webpack.config.* sourceScripts src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress sourceScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch" : "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "chai": "4.2.0",
    "chalk": "2.4.1",
    "cheerio": "1.0.0-rc.2",
    "compression": "^1.7.3",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "eslint": "5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "4.0.2",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "jquery": "^3.3.1",
    "jsdom": "13.0.0",
    "json-schema-faker": "^0.5.0-rc16",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.2",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10"
  }
}
